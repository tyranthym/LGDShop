{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=LGDShop;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "StsConfig": {
    "StsServerIdentityUrl": "__StsServerIdentityUrl__",
    "AngularClientUrl": "__AngularClientUrl__",
    "AngularClientIdTokenOnlyUrl": "__AngularClientIdTokenOnlyUrl__"
  },
  "UseLocalCertStore": "__UseLocalCertStore__",
  "CertificateThumbprint": "__CertificateThumbprint__",
  "EmailSettings": {
    "SendGridApiKey": "_in_local_secret",
    "SenderEmailAddress": "in_local_secret"
  }
}